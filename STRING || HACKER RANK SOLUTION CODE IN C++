 #include <bits/stdc++.h>

using namespace std;





int main() {



    string str1,str2;

    cin>>str1>>str2;

    int s1,s2;

    s1=str1.length();

    s2=str2.length();

    cout<<s1<<" "<<s2<<"\n";

    string str3=str1+str2;

    cout<<str3<<endl;

    int temp=str1[0];

    str1[0]=str2[0];

    str2[0]=temp;

    cout<<str1<<" "<<str2;



    return 0;

}
