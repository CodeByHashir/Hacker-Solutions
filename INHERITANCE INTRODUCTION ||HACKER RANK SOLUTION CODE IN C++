#include <bits/stdc++.h>

using namespace std;

class Triangle{

    public:

        void triangle(){

            cout<<"\nI am a triangle";

        }

};

class Isosceles : public Triangle{

    public:

        void isosceles(){

            cout<<"I am an isosceles triangle\nIn an isosceles triangle two sides are equal";

        }

};

int main()

{

  

    Isosceles isc;

    isc.isosceles();

    isc.triangle();

    return 0;

}

     
