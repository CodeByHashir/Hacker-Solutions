 



#include <bits/stdc++.h>

using namespace std;

int max_of_four_num(int a,int b,int c,int d)

{

    int z;

   if(a>b && a>c && a>d)

    z=a;

   else if(b>a && b>c && b>d)

   z= b;

   else if(c>b && c>a && c>d)

   z= c;

   else if(d>b && d>c && d>a)

   z= d;

   return z;

}

int main()

{

    int a,b,c,d;

    cin>>a>>b>>c>>d;

   cout<<max_of_four_num(a,b,c,d);

    return 0;

}

